{
  "name": "react-embed",
  "version": "3.3.3",
  "description": "",
  "author": {
    "name": "streamich",
    "url": "https://github.com/streamich"
  },
  "homepage": "https://github.com/streamich/react-embed",
  "repository": "streamich/react-embed",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib/",
    "esm/"
  ],
  "scripts": {
    "start": "yarn storybook",
    "prettier": "prettier --ignore-path .gitignore --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:diff": "prettier -l 'src/**/*.{ts,tsx,js,jsx}'",
    "tslint": "tslint 'src/**/*.{js,jsx,ts,tsx}' -t verbose",
    "test": "echo 'Testing is not configured...'",
    "release": "semantic-release",
    "clean": "rimraf lib esm",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn build:cjs && yarn build:es",
    "storybook": "start-storybook -p 6010",
    "storybook:build": "build-storybook",
    "storybook:clean": "rimraf storybook-static"
  },
  "keywords": [],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.1.1",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-backgrounds": "6.4.9",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.4.9",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-options": "5.3.21",
    "@storybook/addon-viewport": "6.4.9",
    "@storybook/react": "6.4.9",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "babel-loader": "8.2.3",
    "fork-ts-checker-webpack-plugin": "6.5.0",
    "git-cz": "4.8.0",
    "husky": "6.0.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.2",
    "storybook-readme": "5.0.9",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "tslint": "5.20.1",
    "tslint-config-common": "1.6.0",
    "typescript": "4.3.5",
    "url-loader": "4.1.1"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {
    "p4-css": "^1.5.1",
    "react-instagram-embed": "^1.5.0",
    "react-player": "^1.15.3",
    "react-simple-player": "^1.0.3",
    "react-youtube": "^7.13.1",
    "scriptjs": "^2.5.9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && yarn tslint",
      "pre-push": "yarn prettier:diff"
    }
  }
}
