{
  "name": "react-embed",
  "version": "2.7.1",
  "description": "",
  "author": {
    "name": "streamich",
    "url": "https://github.com/streamich"
  },
  "homepage": "https://github.com/streamich/react-embed",
  "repository": "streamich/react-embed",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "lib/",
    "esm/"
  ],
  "scripts": {
    "start": "yarn storybook",
    "prettier": "prettier --ignore-path .gitignore --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:diff": "prettier -l 'src/**/*.{ts,tsx,js,jsx}'",
    "tslint": "tslint 'src/**/*.{js,jsx,ts,tsx}' -t verbose",
    "test": "jest --no-cache --config='jest.config.js'",
    "release": "semantic-release",
    "clean": "rimraf lib esm",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "build": "yarn build:cjs && yarn build:es",
    "storybook": "start-storybook -p 6010",
    "storybook:build": "build-storybook",
    "storybook:clean": "rimraf storybook-static"
  },
  "keywords": [],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "@semantic-release/npm": "5.2.0",
    "@storybook/addon-actions": "5.2.3",
    "@storybook/addon-backgrounds": "5.2.3",
    "@storybook/addon-knobs": "5.2.3",
    "@storybook/addon-links": "5.2.3",
    "@storybook/addon-notes": "5.2.3",
    "@storybook/addon-options": "5.2.3",
    "@storybook/addon-viewport": "5.2.3",
    "@storybook/react": "5.2.3",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "babel-loader": "8.0.6",
    "fork-ts-checker-webpack-plugin": "0.5.2",
    "git-cz": "1.8.4",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "rimraf": "2.7.1",
    "semantic-release": "15.13.24",
    "storybook-readme": "4.0.5",
    "ts-jest": "24.1.0",
    "ts-loader": "5.4.5",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-common": "1.6.0",
    "typescript": "3.6.4",
    "url-loader": "1.1.2"
  },
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {
    "p4-css": "^1.1.0",
    "react-instagram-embed": "^1.5.0",
    "react-player": "^1.11.0",
    "react-youtube": "^7.9.0",
    "scriptjs": "^2.5.9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged && yarn tslint",
      "pre-push": "yarn prettier:diff"
    }
  }
}
